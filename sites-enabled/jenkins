server {
        error_log   /var/log/nginx/error.log error;
        listen      80;
        server_name build.openrov.com;

        location ~* (/jenkins) {
           expires epoch;
           proxy_cache_bypass 1;
           proxy_pass http://build.openrov.com:8080;
        }

	location ~* (/debian) {
           expires epoch;
           proxy_cache_bypass 1;
           proxy_pass http://openrov-deb-repository.s3.amazonaws.com;
	}

        location / {
            proxy_pass http://build.openrov.com:8080;
        }

}
